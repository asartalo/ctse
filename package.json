{
  "name": "ctse",
  "version": "0.1.0",
  "description": "Continuous testing wrapper for Selenium",
  "main": "index.js",
  "scripts": {
    "lint-watch": "esw --color -w --clear",
    "install-selenium": "selenium-standalone install",
    "test-watch": "_mocha --watch './**/*.test.js'",
    "test": "nyc _mocha --reporter=dot './**/*.test.js'"
  },
  "keywords": [
    "continuous",
    "testing",
    "selenium",
    "end-to-end"
  ],
  "author": "Wayne Duran",
  "bin": {
    "ctse": "./cli.js"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^4.0.1",
    "node-ipc": "^9.1.1",
    "selenium-standalone": "^6.17.0",
    "selenium-webdriver": "^4.0.0-alpha.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chokidar": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-watch": "^6.0.1",
    "ip": "^1.1.5",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1"
  }
}
